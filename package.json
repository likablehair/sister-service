{
  "name": "@likable-hair/sister-service",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "description": "Integration with the Italian SISTER (Sistema Interscambio Territorio)",
  "scripts": {
    "dev": "vitest",
    "build": "tsc -p tsconfig.build.json && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "format": "prettier --write .",
    "prepare": "husky",
    "test": "vitest run"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/sister-service.js",
      "require": "./dist/sister-service.cjs"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.5.0",
    "@types/node": "^22.5.4",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-prettier": "^5.2.0",
    "globals": "^15.14.0",
    "husky": "^9.1.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/likablehair/sister-service.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/likablehair/sister-service/issues"
  },
  "homepage": "https://github.com/likablehair/sister-service#readme",
  "dependencies": {
    "puppeteer": "^24.37.4",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  }
}
